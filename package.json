{
  "name": "@artisnull/gin",
  "version": "1.1.2",
  "description": "Library that handles state management, business logic, and api calls in an easy-to-use manner.",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "NODE_ENV=production npx webpack && yarn declarations",
    "dev": "NODE_ENV=development npx webpack --watch",
    "declarations": "NODE_ENV=production npx tsc -p ./tsconfig.json",
    "test": "jest",
    "test-watch": "jest --watch",
    "test-coverage": "jest --coverage",
    "see-coverage": "open coverage/lcov-report/index.html",
    "prepublish": "yarn build"
  },
  "keywords": [
    "state",
    "management",
    "fetch",
    "api",
    "business",
    "logic",
    "redux",
    "react",
    "spa"
  ],
  "author": "Zachariah Lambert",
  "license": "MIT",
  "dependencies": {
    "core-js": "3.x",
    "shortid": "^2.2.16"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-typescript": "^7.14.5",
    "@types/jest": "^24.9.1",
    "@types/node": "^13.13.52",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.4.0",
    "fast-safe-stringify": "^2.0.8",
    "graphql": "^15.5.1",
    "jest": "^27.0.6",
    "parcel-bundler": "^1.12.5",
    "prettier": "^1.19.1",
    "release-it": "^14.10.1",
    "typescript": "^3.9.10",
    "webpack": "^5.44.0",
    "webpack-cli": "^4.7.2"
  },
  "jest": {
    "testMatch": [
      "**/__tests__/**/*.js?(x)",
      "**/?(*.)(spec|test).js?(x)",
      "**/tests.(js|jsx|ts|tsx)",
      "**/*.tests.(js|jsx|ts|tsx)"
    ],
    "collectCoverageFrom": [
      "src/**/*.(js|jsx|ts|tsx)"
    ],
    "setupFiles": [
      "<rootDir>/config/jest-setup.js"
    ],
    "coverageDirectory": "./coverage",
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    }
  },
  "browserslist": "> 0.25%, not dead"
}
